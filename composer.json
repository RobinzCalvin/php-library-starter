{
    "name": "ramsey/php-library-skeleton",
    "type": "project",
    "description": "A tool to quickly set up the base files of a PHP library package.",
    "keywords": [
        "skeleton",
        "package",
        "library"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Ramsey",
            "email": "ben@benramsey.com",
            "homepage": "https://benramsey.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "ext-json": "*",
        "symfony/filesystem": "^5",
        "symfony/finder": "^5",
        "symfony/process": "^5",
        "twig/twig": "^3"
    },
    "require-dev": {
        "ramsey/devtools": "^1.5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "ramsey/devtools": {
            "command-prefix": "dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Ramsey\\Skeleton\\": "src/Skeleton/",
            "Vendor\\SubNamespace\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ramsey\\Test\\Skeleton\\": "tests/Skeleton/",
            "Vendor\\Console\\": "resources/console/",
            "Vendor\\Test\\SubNamespace\\": "tests/"
        },
        "files": [
            "vendor/hamcrest/hamcrest-php/hamcrest/Hamcrest.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
